{
  "name": "@kodepandai/whatsapp-api",
  "version": "0.0.1",
  "description": "Whatsapp Api wrapper for NodeJs",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "hook": "node ./webhook/server.mjs"
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
